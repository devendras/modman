    <div class="socialCtas">
        <%= partial "modules/square_cta", :locals => { :color => 'blue' } %>
        <%= partial "modules/social_cta", :locals => { :network => 'facebook' } %>
        <%= partial "modules/social_cta", :locals => { :network => 'pinterest' } %>
